"\n        placeholder=\"请输入微信联想词...\"\n        style=\"width: 50%\"\n        @keyup.enter.native=\"suresearch\"\n      >\n        <i-button slot=\"append\" icon=\"ios-search\" @click=\"suresearch\"></i-button>\n      </i-input>\n    </div>\n    <div class=\"table\" v-if=\"showdata.length>0\">\n      <Table v-if=\"!detail\" border :data=\"showdata\" :columns=\"columns\"></Table>\n      <Table v-if=\"detail\" border :columns=\"columns2\" :data=\"detailData\"></Table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { getWechatThink, getWechatThinkWord } from \"@/api/index\";\nexport default {\n  data() {\n    return {\n      searchWord: \"传奇\",\n      showdata: [],\n      detailData: [],\n      detail: false,\n      columns: [\n        {\n          title: \"联想词\",\n          key: \"word\",\n          render: (h, params) => {\n            return h(\n              \"a\",\n              {\n                style: {\n                  textDecoration: \"underline\"\n                },\n                on: {\n                  click: () => {\n                    getWechatThinkWord({ word: params.row.word }).then(res => {\n                      if (res.code == 1) {\n                        this.detail = true;\n                        this.detailData = res.data;\n                        this.$Message.success(res.message);\n                      } else {\n                        this.$Message.error(res.message);\n                      }\n                    });\n                  }\n                }\n              },\n              params.row.word\n            );\n          }\n        },\n        {\n          title: \"权重\",\n          key: \"suggestion_buf\"\n        },\n        {\n          title: \"类型\",\n          key: \"trie_type\"\n        },\n        {\n          title: \"更新时间\",\n          key: \"update_time\"\n        }\n      ],\n      columns2: [\n        {\n          title: \"联想词\",\n          key: \"name\"\n        }\n      ]\n    };\n  },\n  methods: {\n    suresearch() {\n      if (this.searchWord.length == 0) this.$Message.error(\"输入内容不能为空\");\n      else {\n        this.showdata = [];\n        this.detail = false;\n        getWechatThink({ keyword: this.searchWord }).then(res => {\n          if (res.code == 1) {\n            this.showdata = res.data;\n            this.$Message.success(res.message);\n          } else this.$Message.error(res.message);\n        });\n      }\n    }\n  },\n  mounted() {\n    // getWechatThink({ keyword: this.searchWord }).then(res => {\n    //   if (res.code == 1) {\n    //     this.showdata = res.data;\n    //     this.$Message.success(res.message);\n    //   } else this.$Message.error(res.message);\n    // });\n    // getWechatThinkWord({ word: \"传奇\" }).then(res => {\n    //   console.log(res);\n    // });\n  }\n};\n</script>\n\n<style scoped>\n.main {\n  width: 90%;\n  height: 20%;\n  margin: 0 auto;\n  background: #fff;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n  padding: 20px 0;\n}\n.main p:nth-child(1) {\n  margin-bottom: 15px;\n  font-weight: bold;\n  font-size: 18px;\n}\n.table {\n  width: 90%;\n  margin: 20px auto;\n}\n</style>\n"]}]}