      this.showApp = true;\n                      this.AppId = params.row.searchapp;\n                    }\n                  }\n                },\n                \"详情\"\n              )\n            ]);\n          }\n        }\n      ],\n      showdata: []\n    };\n  },\n  mounted() {\n\n     this.language = true;\n      this.getData(1)\n  },\n  watch:{\n    language(val){\n      this.language=val;\n      this.getData(1);\n\n    }\n  },\n  methods: {\n    getData(i) {\n      this.loading = true;\n      getKeywordApp({ page: i ,language: this.language ? \"tl\" : \"vn\"}).then(res => {\n        if (res.code == 1) {\n          this.showdata = res.msg.data;\n          this.total = res.msg.pageCount * this.pageSize;\n          this.pageindex=i\n          this.loading = false;\n        }\n      });\n    },\n    changePage(index) {\n      this.getData(index);\n    }\n  }\n};\n</script>\n\n<style>\n.key {\n  width: 100%;\n  height: 100%;\n  padding: 10px;\n}\n.page {\n  margin-top: 15px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.app {\n  word-break: break-all;\n}\n.words{\n  display: flex;\njustify-content: flex-start;\nalign-items: center;\n}\n.words p {\n  margin-right: 20px;\n}\n</style>\n"]}]}